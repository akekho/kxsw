{
    "server":["[::0]", "0.0.0.0"]
   ,"server_port":{{ ss_server_port }}
   ,"local_port":1080
   ,"password":"{{ ss_password }}"
   ,"method":"{{ ss_encryption_method }}"
   ,"timeout":{{ ss_timeout }}
{% if ss_fast_open is defined and ansible_kernel | version_compare('3.7', '>=') %}
   ,"fast_open": "{{ ss_fast_open }}"
{% endif %}
{% if ss_dns_addr is defined %}
   ,"addr": "{{ ss_dns_addr }}"
{% endif %}
}
